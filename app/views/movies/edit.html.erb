<h1>Edit <% @movie.title %><h1>

<%= form_for @movie do |f| %>
    <%= f.label :picture %>
    <%= f.text_field :picture %>
  
    <%= f.label :title %>
    <%= f.text_field :title %>
  
    <%= f.label :genre %>
    <%= f.text_field :genre %>

    <%= f.label :year %>
    <%= f.text_field :year %>

    <%= f.label :synopsis %>
    <%= f.text_field :synopsis %>
  
    <%= f.submit "Edit" %>
    
  <% end %>